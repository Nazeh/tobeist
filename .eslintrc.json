{
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "plugins": [
    "svelte3",
    "prettier"
  ],
  "extends": ["prettier"],
  "overrides": [{
    "files": ["**/*.svelte"],
    "processor": "svelte3/svelte3"
  }],
  "rules": {
    "quotes": ["error", "single"],
    "no-multiple-empty-lines": "error"
  }
}